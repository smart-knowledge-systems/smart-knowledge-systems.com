{
  "name": "com.smart-knowledge-systems.www",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "bun run --bun eslint . --cache",
    "lint:fix": "bun run --bun eslint --fix .",
    "typecheck": "bun run --bun tsc --noEmit",
    "check": "bun run --bun eslint . --cache && tsc --noEmit",
    "format": "bun run --bun prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,css}\" --cache -l",
    "format:check": "bun run --bun prettier --check \"**/*.{ts,tsx,js,jsx,md,mdx,css}\" --cache",
    "sequoia:publish": "sequoia publish",
    "sequoia:inject": "sequoia inject --output .next",
    "build:full": "bun run sequoia:publish && bun run build && bun run sequoia:inject",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@axiomhq/js": "^1.4.0",
    "@axiomhq/logging": "^0.2.0",
    "@axiomhq/nextjs": "^0.2.0",
    "@axiomhq/react": "^0.2.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@preact/signals": "^2.8.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toggle": "^1.1.10",
    "@tailwindcss/typography": "^0.5.19",
    "@vercel/analytics": "^1.6.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.575.0",
    "next": "^16.1.6",
    "next-themes": "^0.4.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "rehype-pretty-code": "^0.14.1",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.22.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@tailwindcss/postcss": "^4.2.0",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.3",
    "eslint-config-next": "^16.1.6",
    "mini-css-extract-plugin": "^2.10.0",
    "prettier": "^3.8.1",
    "sequoia-cli": "^0.5.0-alpha.0",
    "shadcn": "^3.8.5",
    "tailwindcss": "^4.2.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
